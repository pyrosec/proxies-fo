{
  "name": "proxies-fo",
  "type": "module",
  "version": "0.1.0",
  "scripts": {
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:commonjs": "tsc -p tsconfig.commonjs.json",
    "build": "npm-run-all build:esm build:commonjs",
    "prettier": "prettier --write ./src.ts/**/*.ts"
  },
  "bin": {
    "proxies-fo": "./bin/proxies-fo.cjs"
  },
  "exports": {
    ".": {
      "import": "./lib.esm/index.js",
      "default": "./lib.commonjs/index.js"
    },
    "./cli": {
      "import": "./lib.esm/cli.js",
      "default": "./lib.commonjs/cli.js"
    }
  },
  "description": "proxies.fo API client TypeScript library and CLI tool",
  "main": "lib/index.js",
  "repository": "https://github.com/pyrosec/proxies-fo",
  "author": "pyrosec",
  "license": "MIT",
  "private": false,
  "dependencies": {
    "change-case": "^4.1.2",
    "node-fetch": "2",
    "winston": "^3.8.2",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@types/node": "^20.1.1",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.0.4"
  }
}
